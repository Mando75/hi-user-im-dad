{
  "name": "remix-template-netlify",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development netlify dev",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "start": "cross-env NODE_ENV=production netlify dev",
    "prepare": "husky install",
    "db:prepare": "npm run db:push && npm run db:generate && npm run db:seed",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "node prisma/seed"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@justinwaite/tailwind-ui": "^1.8.0",
    "@netlify/functions": "^1.0.0",
    "@prisma/client": "^3.14.0",
    "@remix-run/netlify": "^1.5.1",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-auth": "^3.2.2",
    "remix-auth-github": "^1.1.1",
    "remix-validated-form": "^4.5.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@tailwindcss/forms": "^0.5.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.15.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.14.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
