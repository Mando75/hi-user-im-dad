{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "deploy": "fly deploy --remote-only --build-arg NODE_AUTH_TOKEN=$NODE_AUTH_TOKEN",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "test": "run-p test:*",
    "test:ts": "tsc",
    "test:lint": "run-p lint:*",
    "start": "remix-serve build",
    "prepare": "husky install",
    "db:prepare": "npm run db:push && npm run db:generate && npm run db:seed",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "node prisma/seed",
    "lint:js": "eslint . --cache --max-warnings=0",
    "lint:js:fix": "eslint . --cache --fix"
  },
  "dependencies": {
    "@heroicons/react": "1.0.6",
    "@icons-pack/react-simple-icons": "5.1.2",
    "@mando-collabs/tailwind-ui": "1.15.2",
    "@netlify/functions": "1.0.0",
    "@prisma/client": "4.0.0",
    "@remix-run/node": "1.6.3",
    "@remix-run/react": "1.6.3",
    "@remix-run/serve": "1.6.3",
    "@remix-validated-form/with-zod": "2.0.1",
    "@travi/any": "2.1.5",
    "classnames": "2.3.1",
    "http-status-codes": "2.2.0",
    "pusher": "5.0.1",
    "pusher-js": "7.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "remix-auth": "3.2.2",
    "remix-auth-github": "1.1.1",
    "remix-utils": "3.3.0",
    "remix-validated-form": "4.5.0",
    "zod": "3.17.3",
    "zod-form-data": "1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.6.3",
    "@remix-run/eslint-config": "1.6.3",
    "@tailwindcss/forms": "0.5.2",
    "@types/pusher-js": "5.1.0",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-tailwindcss": "3.6.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prisma": "4.0.0",
    "tailwindcss": "3.1.5",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
